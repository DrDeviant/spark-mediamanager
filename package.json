{
  "name": "react-redux-electron",
  "version": "0.1.0",
  "description": "An electron boilerplate providing support for React, React Hot Reload 3, Redux, React Router and Webpack.",
  "main": "main.js",
  "repository": "https://github.com/GabrielQDuarte/react-webpack-minimal.git",
  "author": "Gabriel Duarte <gabrielqduarte@gmail.com> (https://github.com/GabrielQDuarte)",
  "license": "MIT",
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently \"npm run start-server\" \"electron -r babel-register -r babel-polyfill .\" --kill-others -s first",
    "start-server": "cross-env NODE_ENV=development node server",
    "lint": "standard | snazzy",
    "lint-fix": "standard --fix | snazzy",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress --colors",
    "build-dev": "cross-env NODE_ENV=development webpack --config webpack.config.dev.js --progress --colors"
  },
  "// ALERT //": "Change the 'standard-loader' package below to just 'standard-loader' (without the 'GabrielDuarteM/' prefix) when this pull request is accepted: https://github.com/timoxley/standard-loader/pull/72",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-intl": "^2.3.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "clean-webpack-plugin": "^0.1.14",
    "concurrently": "^3.1.0",
    "cross-env": "3.1.3",
    "css-loader": "^0.26.1",
    "devtron": "^1.4.0",
    "electron": "^1.4.13",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.0.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "react-hot-loader": "next",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "redux-slider-monitor": "^2.0.0-0",
    "snazzy": "^5.0.0",
    "standard": "^8.6.0",
    "standard-loader": "GabrielDuarteM/standard-loader",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "material-ui": "^0.18.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-intl": "^2.2.2",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0"
  }
}
